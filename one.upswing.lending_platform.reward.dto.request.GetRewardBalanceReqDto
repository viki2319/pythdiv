java
package one.upswing.lending_platform.reward.dto.request;

import javax.validation.constraints.NotBlank;
import javax.validation.constraints.Size;

public class GetRewardBalanceReqDto {

    @NotBlank(message = "AccountId cannot be blank")
    @Size(min = 1, max = 255, message = "AccountId must be between 1 and 255 characters")
    private String accountId;

    public GetRewardBalanceReqDto() {
    }

    public GetRewardBalanceReqDto(String accountId) {
        this.accountId = accountId;
    }

    public String getAccountId() {
        return accountId;
    }

    public void setAccountId(String accountId) {
        this.accountId = accountId;
    }

    @Override
    public String toString() {
        return "GetRewardBalanceReqDto{" +
                "accountId='" + accountId + '\'' +
                '}';
    }
}